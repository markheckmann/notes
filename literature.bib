}

@article{mcdonagh_implication_1987,
	title = {The implication potential of personal constructs in relation to their subjective importance and order of elicitation},
	volume = {15},
	copyright = {This website and its contents are copyright-protected under New Zealand and International laws. No duplication or transmission in any form, print, electronic, or otherwise, is allowed without permission.  Authors who publish with this journal agree to the following terms:  Copyright of all articles published in  {SBP} Journal  are the property of Society for Personality Research.  Permission may be granted on case by case basis. For permission write to: editor@sbp-journal.com  When  {SBP}  article extracts are published elsewhere our policy is to not charge any fee, however there are two conditions; that a full citation be included: that is, name of author/s, title of article, journal title (Social Behavior and Personality: An international journal), year of publication, number of volume and issue (e.g., 31(3)), and finally page numbers (e.g., 540-550), and 2) that notification of such citations be sent to us at editor@sbp-journal.com.},
