@article{adachi_joint_2009,
	title = {Joint Procrustes Analysis for Simultaneous Nonsingular Transformation of Component Score and Loading Matrices},
	volume = {74},
	issn = {0033-3123, 1860-0980},
	url = {http://link.springer.com/article/10.1007/s11336-009-9131-2},
	doi = {10.1007/s11336-009-9131-2},
	abstract = {In component analysis solutions, post-multiplying a component score matrix by a nonsingular matrix can be compensated by applying its inverse to the corresponding loading matrix. To eliminate this indeterminacy on nonsingular transformation, we propose Joint Procrustes Analysis ({JPA}) in which component score and loading matrices are simultaneously transformed so that the former matrix matches a target score matrix and the latter matches a target loading matrix. The loss function of {JPA} is a function of the nonsingular transformation matrix and its inverse, and is difficult to minimize straightforwardly. To deal with this difficulty, we reparameterize those matrices by their singular value decomposition, which reduces the minimization to alternately solving quartic equations and performing the existing multivariate procedures. This algorithm is assessed in a simulation study. We further extend {JPA} for cases where targets are linear functions of unknown parameters. We also discuss how the application of {JPA} can be extended in different fields.},
	language = {en},
	number = {4},
	urldate = {2014-08-07},
	journal = {Psychometrika},
	author = {Adachi, Kohei},
	month = dec,
	year = {2009},
	keywords = {Assessment, Testing and Evaluation, component analysis, joint Procrustes analysis, nonsingular transformation, Psychometrics, singular value decomposition, Statistical Theory and Methods, Statistics for Social Science, Behavorial Science, Education, Public Policy, and Law},
	pages = {667--683},
	file = {Snapshot:/Users/mark/Library/Application Support/Zotero/Profiles/3djrw6rs.default/zotero/storage/JFMV4H4M/10.html:text/html}
}

@article{adachi_generalized_2013,
	title = {Generalized joint Procrustes analysis},
	volume = {28},
	issn = {0943-4062, 1613-9658},
	url = {http://link.springer.com/article/10.1007/s00180-013-0413-x},
	doi = {10.1007/s00180-013-0413-x},
	abstract = {In this paper, we propose a generalized version of Adachi’s (Psychometrika 74:667–683, 2009) Joint Procrustes Analysis ({GJPA}), in order to transform the principal component score and loading matrices obtained for multiple data sets of the same size. The transformation is made so that multiple score and loading matrices are optimally matched to two unknown target matrices, respectively, without affecting the fit of the score and loading matrices to data sets, and without any constraint imposed on the transformation, except for its being nonsingular. The resulting transformed score and loading matrices can reasonably be compared across data sets. A simulation study is performed for assessing an alternate least-squares algorithm for {GJPA}. Additional procedures for interpreting {GJPA} solutions are also presented and they are illustrated with a real data example. Finally, {GJPA} is reconsidered in the contexts of three-way data analysis and canonical correlation analysis.},
	language = {en},
	number = {6},
	urldate = {2014-08-07},
	journal = {Computational Statistics},
	author = {Adachi, Kohei},
	month = dec,
	year = {2013},
	keywords = {Canonical correlation analysis, Economic Theory, Multiple data sets, nonsingular transformation, Principal component analysis, Probability and Statistics in Computer Science, Probability Theory and Stochastic Processes, Procrustes analysis, Statistics, general},
	pages = {2449--2464},
	file = {Snapshot:/Users/mark/Library/Application Support/Zotero/Profiles/3djrw6rs.default/zotero/storage/8QIAZHD2/10.html:text/html}
}

